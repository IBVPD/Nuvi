<!DOCTYPE html>
<html lang="en">
    <head>
	<meta charset="utf-8" />
        <title>{% block pagetitle %}{% endblock %}{%block sitetitle %}{%endblock %}</title>
<!--	<meta name="description" content="{ {page.description} }" />-->
	<meta name="viewport" content="width=device-width, initial-scale=1.0" />

        {% block stylesheets %}
            <!-- basic styles -->
            <link href="{{asset('/bundles/nsace/css/bootstrap.min.css')}}" rel="stylesheet" />

            <link rel="stylesheet" href="{{asset('/bundles/nsace/css/font-awesome.min.css')}}" />
            <!--[if IE 7]>
              <link rel="stylesheet" href="{{asset('/bundles/nsace/css/font-awesome-ie7.min.css')}}" />
            <![endif]-->

            <!-- fonts -->
            
 <!--<link rel="stylesheet" href="{ { {site.protocol } } }//fonts.googleapis.com/css?family=Open+Sans:400,300" />-->
            <link rel="stylesheet" href="{{asset('/bundles/nsace/css/ace-fonts.css')}}" />
 
            <!-- ace styles -->
            <link rel="stylesheet" href="{{asset('/bundles/nsace/css/ace.min.css')}}" />
            <link rel="stylesheet" href="{{asset('/bundles/nsace/css/ace-rtl.min.css')}}" />
            <!--[if lte IE 8]>
              <link rel="stylesheet" href="{{asset('/bundles/nsace/css/ace-ie.min.css')}}" />
            <![endif]-->

            <!-- inline styles related to this page -->
            {% block inlinestyles %}
            {% endblock inlinestyles %}
        {% endblock %}

	<!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
	<!--[if lt IE 9]>
	<script src="{{asset('/bundles/nsace/js/html5shiv.js')}}"></script>
	<script src="{{asset('/bundles/nsace/js/respond.min.js')}}"></script>
	<![endif]-->

</head>
<body class="login-layout">
    <div class="main-container">
        <div class="main-content">
            <div class="row">
                <div class="col-sm-10 col-sm-offset-1">
                {% block body %}
                {% endblock body %}
                </div><!--/.col-->
            </div><!--/.row-->
        </div>
    </div><!--/.main-container-->


    {%block footer %}

    {%endblock footer %}
        
    {% block javascripts %}
        <!--[if !IE]> -->
        <script src="{{app.request.scheme}}://ajax.googleapis.com/ajax/libs/jquery/2.0.3/jquery.min.js"></script>
        <!-- <![endif]-->
        <!--[if IE]>
        <script src="{{app.request.scheme}}//ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js"></script>
        <![endif]-->

        <!--[if !IE]> -->
        <script type="text/javascript">
         window.jQuery || document.write("<script src='{{asset('/bundles/nsace/js/jquery-2.0.3.min.js')}}'>"+"<"+"/script>");
        </script>
        <!-- <![endif]-->
        <!--[if IE]>
        <script type="text/javascript">
         window.jQuery || document.write("<script src='{{asset('/bundles/nsace/js/jquery-1.10.2.min.js')}}'>"+"<"+"/script>");
        </script>
        <![endif]-->

        <script type="text/javascript">
                if("ontouchend" in document) document.write("<script src='{{asset('/bundles/nsace/js/jquery.mobile.custom.min.js')}}'>"+"<"+"/script>");
        </script>    
    {% endblock %}
</body>
</html>
